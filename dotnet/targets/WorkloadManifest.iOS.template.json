{
	"version": "@VERSION@",
	"workloads": {
		"@PLATFORM_LOWERCASE@": {
			"description": ".NET SDK Workload for building @PLATFORM@ applications.",
			"packs": [
				"Microsoft.@PLATFORM@.Sdk.NET7",
				"Microsoft.@PLATFORM@.Sdk.NET6",
				"Microsoft.@PLATFORM@.Windows.Sdk.Aliased",
				"Microsoft.@PLATFORM@.Ref.NET7",
				"Microsoft.@PLATFORM@.Runtime.ios-arm.NET7",
				"Microsoft.@PLATFORM@.Runtime.ios-arm64.NET7",
				"Microsoft.@PLATFORM@.Runtime.iossimulator-x86.NET7",
				"Microsoft.@PLATFORM@.Runtime.iossimulator-x64.NET7",
				"Microsoft.@PLATFORM@.Runtime.iossimulator-arm64.NET7",
				"Microsoft.@PLATFORM@.Runtime.ios-arm",
				"Microsoft.@PLATFORM@.Runtime.ios-arm64",
				"Microsoft.@PLATFORM@.Runtime.iossimulator-x86",
				"Microsoft.@PLATFORM@.Runtime.iossimulator-x64",
				"Microsoft.@PLATFORM@.Runtime.iossimulator-arm64",
				"Microsoft.@PLATFORM@.Templates"
			],
			"extends": [
				"microsoft-net-runtime-ios"
			]
		}
	},
	"packs": {
		"Microsoft.@PLATFORM@.Sdk.NET7": {
			"kind": "sdk",
			"version": "@VERSION@",
			"alias-to": {
				"any": "Microsoft.@PLATFORM@.Sdk",
			}
		},
		"Microsoft.@PLATFORM@.Windows.Sdk.Aliased": {
			"kind": "sdk",
			"version": "@VERSION@",
			"alias-to": {
				"win-x64": "Microsoft.@PLATFORM@.Windows.Sdk",
				"win-x86": "Microsoft.@PLATFORM@.Windows.Sdk",
				"win-arm64": "Microsoft.@PLATFORM@.Windows.Sdk",
			}
		},
		"Microsoft.@PLATFORM@.Ref.NET7": {
			"kind": "framework",
			"version": "@VERSION@",
			"alias-to": {
				"any": "Microsoft.@PLATFORM@.Ref",
			}
		},
		"Microsoft.@PLATFORM@.Runtime.ios-arm.NET7": {
			"kind": "framework",
			"version": "@VERSION@",
			"alias-to": {
				"any": "Microsoft.@PLATFORM@.Runtime.ios-arm",
			}
		},
		"Microsoft.@PLATFORM@.Runtime.ios-arm64.NET7": {
			"kind": "framework",
			"version": "@VERSION@",
			"alias-to": {
				"any": "Microsoft.@PLATFORM@.Runtime.ios-arm64",
			}
		},
		"Microsoft.@PLATFORM@.Runtime.iossimulator-x86.NET7": {
			"kind": "framework",
			"version": "@VERSION@",
			"alias-to": {
				"any": "Microsoft.@PLATFORM@.Runtime.iossimulator-x86",
			}
		},
		"Microsoft.@PLATFORM@.Runtime.iossimulator-x64.NET7": {
			"kind": "framework",
			"version": "@VERSION@",
			"alias-to": {
				"any": "Microsoft.@PLATFORM@.Runtime.iossimulator-x64",
			}
		},
		"Microsoft.@PLATFORM@.Runtime.iossimulator-arm64.NET7": {
			"kind": "framework",
			"version": "@VERSION@",
			"alias-to": {
				"any": "Microsoft.@PLATFORM@.Runtime.iossimulator-arm64",
			}
		},
		"Microsoft.@PLATFORM@.Templates": {
			"kind": "template",
			"version": "@VERSION@"
		},

		"Microsoft.@PLATFORM@.Sdk.NET6": {
			"kind": "sdk",
			"version": "@NET6_VERSION@",
			"alias-to": {
				"any": "Microsoft.@PLATFORM@.Sdk"
			}
		},
		"Microsoft.@PLATFORM@.Ref": {
			"kind": "framework",
			"version": "@NET6_VERSION@"
		},
		"Microsoft.@PLATFORM@.Runtime.ios-arm": {
			"kind": "framework",
			"version": "@NET6_VERSION@"
		},
		"Microsoft.@PLATFORM@.Runtime.ios-arm64": {
			"kind": "framework",
			"version": "@NET6_VERSION@"
		},
		"Microsoft.@PLATFORM@.Runtime.iossimulator-x86": {
			"kind": "framework",
			"version": "@NET6_VERSION@"
		},
		"Microsoft.@PLATFORM@.Runtime.iossimulator-x64": {
			"kind": "framework",
			"version": "@NET6_VERSION@"
		},
		"Microsoft.@PLATFORM@.Runtime.iossimulator-arm64": {
			"kind": "framework",
			"version": "@NET6_VERSION@"
		},
	}
}

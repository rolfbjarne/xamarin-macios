<?xml version="1.0" encoding="utf-8"?>
<Project>
	<PropertyGroup>
		<OutputType>Exe</OutputType>

		<ApplicationTitle>MultiRidApp</ApplicationTitle>
		<ApplicationId>com.xamarin.multiridapp</ApplicationId>
		<ApplicationVersion>1.0</ApplicationVersion>

		<!-- https://github.com/dotnet/runtime/issues/53307 -->
		<UseAppHost>false</UseAppHost>
		<_RuntimeIdentifierUsesAppHost>false</_RuntimeIdentifierUsesAppHost>
	</PropertyGroup>

	<ItemGroup>
		<Compile Include="../*.cs" />
		<None Include="../Info.plist">
			<Link>Info.plist</Link>
		</None>
	</ItemGroup>
</Project>
